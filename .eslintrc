{
  "extends": "airbnb-base",
  "env": {
      "es6": true,
      "browser": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
      "brace-style": [
          "error",
          "stroustrup"
      ],
      "comma-dangle": [
          "error",
          "never"
      ],
      "no-unused-vars": [
          "warn"
      ],
      "no-var": [
          "off"
      ],
      "one-var": [
          "off"
      ],
      "no-console": "off",
      "import/no-named-as-default": "0"
  }
}